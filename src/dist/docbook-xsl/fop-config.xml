<?xml version="1.0" encoding="UTF-8"?>
<fop version="1.0">
  <strict-configuration>true</strict-configuration>
  <renderers>
    <renderer mime="application/pdf">
      <fonts>
        <font kerning="yes" embed-url="fonts/msyhbd.ttf">
          <font-triplet name="WenQuanYi Micro Hei" style="normal" weight="bold"/>
        </font>
        <font kerning="yes" embed-url="fonts/simkai.ttf">
          <font-triplet name="WenQuanYi Micro Hei" style="italic" weight="normal"/>
        </font>
        <font kerning="yes" embed-url="fonts/hwkaibd.ttf">
          <font-triplet name="WenQuanYi Micro Hei" style="italic" weight="bold"/>
        </font>
        <auto-detect />
      </fonts>
    </renderer>
  </renderers>
</fop>
